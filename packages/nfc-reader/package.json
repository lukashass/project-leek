    "@feathersjs/feathers": "4.5.11",
    "@feathersjs/socketio-client": "4.5.11",
    "@leek/commons": "*",
    "evdev": "git://github.com/anbraten/node-evdev.git",
    "socket.io-client": "2.4.0"
  },
  "devDependencies": {
    "@vercel/ncc": "0.26.1",
    "nodemon": "2.0.6",
    "ts-node": "9.1.1"
  }
}
